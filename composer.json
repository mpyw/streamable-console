{
    "name": "mpyw/streamable-console",
    "description": "Call interactive artisan command using arbitrary stream",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "illuminate", "console", "stdin", "input", "interactive"],
    "autoload": {
        "psr-4": {
            "Mpyw\\StreamableConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\StreamableConsole\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0 || ^12.0 || ^13.0",
        "illuminate/contracts": "^11.0 || ^12.0 || ^13.0",
        "guzzlehttp/psr7": "^2.7"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": ">=9.0",
        "phpunit/phpunit": ">=11.0",
        "mockery/mockery": "^1.6.12"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
