{
    "name": "mpyw/streamable-console",
    "description": "Call interactive artisan command using arbitrary stream",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "illuminate", "console", "stdin", "input", "interactive"],
    "autoload": {
        "psr-4": {
            "Mpyw\\StreamableConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\StreamableConsole\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0 || ^10.0 || ^11.0",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0",
        "guzzlehttp/psr7": "^1.7"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": "^4.9 || ^5.9 || >=6.6",
        "phpunit/phpunit": ">=9.5",
        "mockery/mockery": "^1.3.3 || ^1.4.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
